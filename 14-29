27
let month = 7
switch(month) {
    case 1:
    case 2:
    case 12:
        console.log("Зима");
        break;
    case 3:
    case 4:
    case 5:
        console.log("Весна");
        break;
    case 6:
    case 7:
    case 8:
        console.log("Лето");
        break;
    case 9:
    case 10:
    case 11:
        console.log("Осень");
        break;
    default:
        console.log("Ошибка");
    }
    
28
   let A = 5, B = 1
   switch(B) {
    case 1:
        console.log(A * 1);
        break;
    case 2:
        console.log(A * 0.00001*0.1);
        break;
    case 3:
        console.log(A * 0.001);
        break;
    case 4:
        console.log(A * 1000);
        break;
    case 5:
        console.log(A * 100);
        break;
    default:
        console.log("Ошибка")
       }
   
