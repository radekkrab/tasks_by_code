let A=2, B=-3
if(A>=B || B < -2) {
    console.log(`Неравенства A ≥ 0 или B < –2 справедливы, при A=${A} B=${B}`)
    }else{
       console.log(`Неравенства A ≥ 0 или B < –2 ложны, при A=${A} B=${B}`)
        }
        
